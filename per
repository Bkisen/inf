def permutations(pos = 0, res= ''):
    global alp, n, cnt
    if pos == n:
        print(res)
        cnt += 1
        return
    for c in alp: 
        res += str(c)
        permutations(pos + 1, res)
        res = res[:-1]


alp = [4, 1, 3, 5]
n = 2
cnt = 0

permutations()
print('-------\n' + str(cnt))
